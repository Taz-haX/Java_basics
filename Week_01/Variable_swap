public class Main {

	public static void main(String[] args) {
		
		String x = "Verstappening";
		String y = "Hammertime";
		
		String temp = null;
		
		System.out.println("x: "+x);
		System.out.println("y: "+y+"\n");
		
		temp = x;
		x=y;
		y=temp;
		
		System.out.println("x: "+x);
		System.out.println("y: "+y);
				
	}

}
